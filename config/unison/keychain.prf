# Synchronize our keychain

path = usr/keychain

nodeletion = /home/victor

# Keep GPG private keys out
ignore = Name random_seed
ignore = Name secring.gpg
ignore = Name private*

# Ignore ssh private keys
ignore = Name *rsa*
ignorenot = Name *.pub

# Backs up all gpg files
backup = Name *.gpg
backup = Name id_rsa

copyonconflict = true
include common
